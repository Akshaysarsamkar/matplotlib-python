import matplotlib.pyplot as plt
x = ["c", "c++", "java", "python"]
y = [10, 40,20,50]

plt.bar(x, y)
plt. show()#title
import matplotlib.pyplot as plt
x = ["c", "c++", "java", "python"]
y = [10, 40,20,50]

#add xlable or ylable
plt.xlabel ("language",fontsize=22)
plt.ylabel ("no_of_stu",fontsize=20)
plt.title("Programing interest",fontsize=20,color="r")
plt.bar(x, y, color="y",width=0.3)
plt.show()
